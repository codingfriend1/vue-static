{
  "name": "vue-static",
  "description": "A static site generator in Vue.js",
  "version": "0.0.0",
  "homepage": "",
  "main": "server",
  "keywords": [
    "Vue.js",
    "static-site-generator"
  ],
  "author": {
    "name": "Jon Paul Miles",
    "email": "codingfriend1@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server"
  },
  "engines": {
    "node": ">=7.6.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "serve": "http-server --silent -p8080 dist",
    "gulp": "gulp --gulpfile the-magic/build/gulpfile.js",
    "optimize": "node the-magic/build/optimize-images.js",
    "time": "node the-magic/build/get-time.js",
    "start": "npm run gulp && node the-magic/build/server",
    "build:client": "cross-env NODE_ENV=production webpack --config the-magic/build/webpack.client.config.js --no-info --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config the-magic/build/webpack.server.config.js --no-info --hide-modules",
    "build": "rimraf dist && npm run gulp && npm run build:client && npm run build:server && cross-env NODE_ENV=production node the-magic/build/build.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "gray-matter": "^4.0.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.throttle": "^4.1.1",
    "node-fetch": "^2.6.0",
    "route-cache": "^0.4.4",
    "serve-favicon": "^2.5.0",
    "vue": "^2.6.10",
    "vue-analytics": "^5.17.0",
    "vue-meta": "git+https://github.com/codingfriend1/vue-meta.git#dev",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vue-stash": "^2.0.1-beta"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-inject": "^5.0.5",
    "http-server": "^0.11.1",
    "imagemin": "^6.1.0",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-optipng": "^7.0.0",
    "imagemin-webp": "^5.1.0",
    "json-loader": "^0.5.7",
    "klaw": "^3.0.0",
    "markdown-it": "^8.4.2",
    "markdown-it-attrs": "^2.4.1",
    "markdown-it-gallery": "^0.1.6",
    "markdown-it-modify-token": "^1.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "progress-bar-webpack-plugin": "^1.12.1",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^2.0.1",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
